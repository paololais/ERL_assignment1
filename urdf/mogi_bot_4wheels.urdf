<?xml version="1.0"?>
<robot name="mogi_bot_4wheels" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- keep includes for gazebo plugin fragment and materials if present -->
  <xacro:include filename="$(find assignment1)/urdf/mogi_bot_4wheels.gazebo" />
  <xacro:include filename="$(find assignment1)/urdf/materials.xacro" />

  <!-- base footprint -->
  <link name="base_footprint" />

  <!-- base chassis / base_link -->
  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_footprint" />
    <child link="base_link" />
  </joint>

  <link name="base_link">
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0" />
      <origin xyz="0.0 0 0" rpy="0 0 0" />
      <inertia
        ixx="0.0625" ixy="0" ixz="0"
        iyy="0.2125" iyz="0"
        izz="0.25"
      />
    </inertial>

    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size=".4 .2 .1" />
      </geometry>
    </collision>

    <visual name="base_link_visual">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://assignment1/meshes/mogi_bot.dae" />
      </geometry>
    </visual>

     <collision name='rear_caster_collision'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0" />
      <geometry>
        <sphere radius="0.0499" />
      </geometry>
    </collision>

    <visual name='rear_caster_visual'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0" />
      <geometry>
        <sphere radius="0.05" />
      </geometry>
    </visual>

    <collision name='front_caster_collision'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0" />
      <geometry>
        <sphere radius="0.0499" />
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0" />
      <geometry>
        <sphere radius="0.05" />
      </geometry>
    </visual>

  </link>

  <!-- FRONT LEFT WHEEL-->
  <joint type="continuous" name="front_left_wheel_joint">
    <origin xyz="0.15 0.15 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="front_left_wheel" />
    <axis xyz="0 1 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="front_left_wheel">
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia
        ixx="0.014" ixy="0" ixz="0"
        iyy="0.014" iyz="0"
        izz="0.025" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius=".1" length=".05" />
      </geometry>
    </collision>

    <visual name="front_left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <mesh filename="package://assignment1/meshes/wheel.dae" />
      </geometry>
    </visual>
  </link>

  <!-- FRONT RIGHT WHEEL -->
  <joint type="continuous" name="front_right_wheel_joint">
    <origin xyz="0.15 -0.15 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="front_right_wheel" />
    <axis xyz="0 1 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="front_right_wheel">
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia
        ixx="0.014" ixy="0" ixz="0"
        iyy="0.014" iyz="0"
        izz="0.025" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius=".1" length=".05" />
      </geometry>
    </collision>

    <visual name="front_right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <mesh filename="package://assignment1/meshes/wheel.dae" />
      </geometry>
    </visual>
  </link>

  <!-- BACK LEFT WHEEL -->
  <joint type="continuous" name="back_left_wheel_joint">
    <origin xyz="-0.15 0.15 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="back_left_wheel" />
    <axis xyz="0 1 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="back_left_wheel">
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia
        ixx="0.014" ixy="0" ixz="0"
        iyy="0.014" iyz="0"
        izz="0.025" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius=".1" length=".05" />
      </geometry>
    </collision>

    <visual name="back_left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <mesh filename="package://assignment1/meshes/wheel.dae" />
      </geometry>
    </visual>
  </link>

  <!-- BACK RIGHT WHEEL -->
  <joint type="continuous" name="back_right_wheel_joint">
    <origin xyz="-0.15 -0.15 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="back_right_wheel" />
    <axis xyz="0 1 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="back_right_wheel">
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia
        ixx="0.014" ixy="0" ixz="0"
        iyy="0.014" iyz="0"
        izz="0.025" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius=".1" length=".05" />
      </geometry>
    </collision>

    <visual name="back_right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <mesh filename="package://assignment1/meshes/wheel.dae" />
      </geometry>
    </visual>
  </link>

  <!-- Gazebo friction / tuning for each wheel -->
  <gazebo reference="front_left_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>

  <gazebo reference="front_right_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>

  <gazebo reference="back_left_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>

  <gazebo reference="back_right_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>

  <gazebo reference="base_link">
    <mu1>0.000002</mu1>
    <mu2>0.000002</mu2>
  </gazebo>

  <link name="camera_link">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia
        ixx="1e-6" ixy="0" ixz="0"
        iyy="1e-6" iyz="0"
        izz="1e-6"
      />
    </inertial>

    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size=".03 .03 .03" />
      </geometry>
    </collision>

    <visual name="camera_link_visual">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size=".03 .03 .03" />
      </geometry>
    </visual>
  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.225 0 0.075" rpy="0 0 0" />
    <child link="camera_link" />
    <parent link="base_link" />
    <axis xyz="0 1 0" />
  </joint>

  <joint type="fixed" name="camera_optical_joint">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707" />
    <child link="camera_link_optical" />
    <parent link="camera_link" />
  </joint>

  <link name="camera_link_optical">
  </link>
</robot>